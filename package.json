{
  "name": "setup-spark",
  "version": "1.0.1",
  "description": "Setup Spark action",
  "main": "dist/index.js",
  "scripts": {
    "tsc": "tsc",
    "ncc": "ncc build lib/setup-spark.js",
    "build": "npm run fmt && tsc && ncc build lib/setup-spark.js",
    "dev": "npm run build && act -j test-setup-spark",
    "licensed": "./licensed cache",
    "fmt": "prettier --write **/*.ts",
    "lint": "prettier --check **/*.ts && ./licensed status",
    "test": "jest"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/vemonet/setup-spark.git"
  },
  "keywords": [
    "actions",
    "node",
    "setup",
    "apache",
    "spark"
  ],
  "author": "vemonet",
  "license": "MIT",
  "dependencies": {
    "@actions/core": "^1.10.0",
    "@actions/io": "^1.1.3",
    "@actions/tool-cache": "^2.0.1"
  },
  "devDependencies": {
    "@types/jest": "^27.5.0",
    "@types/node": "^12.20.19",
    "@vercel/ncc": "^0.36.1",
    "jest": "^28.1.0",
    "jest-circus": "^28.1.0",
    "prettier": "^2.3.2",
    "ts-jest": "^28.0.2",
    "typescript": "^5.0.3"
  },
  "prettier": {
    "printWidth": 120,
    "tabWidth": 2,
    "useTabs": false,
    "semi": true,
    "singleQuote": true,
    "trailingComma": "none",
    "bracketSpacing": false,
    "arrowParens": "avoid",
    "parser": "typescript"
  }
}
